/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a=b,b=b,out=c);
    Nand(a=a,b=c,out=e);
    Nand(a=a,b=a,out=d);
    Nand(a=d,b=b,out=f);
    Nand(a=e,b=f,out=out);
}